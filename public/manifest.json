{


  "manifest_version": 2,
  "name": "My Extension",
  "version": "0.0.1",

  "browser_action": {
    "default_popup": "index.html",
    "default_title": "Open the popup"
  },
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },


  "description": "A plain text description",
  "content_security_policy": "script-src 'self' 'sha256-skgncRC5tpQtb7ELhg8ogoMEbMqtIQE9KnIzZKPfbPQ='; object-src 'self'",  
  "permissions": [ "tabs"
  ]
}
